@charset "utf-8";
*{
		margin: 0;
		padding: 0;
		font-family:"微软雅黑";
	}
	a{text-decoration: none;color:#FFF}
	.clear{clear:both;}
    
	.bigdiv{ padding:0; margin:0;width:100vw; height:100vh; overflow:hidden;background:url(../images/index3.png);background-size:100vw 100vh}
	.list_left{width:20vw;height:100vh;float:left; border-right:2px solid #999;background-color:rgba(255, 255, 255, 0.2);color:#FFF}
	.title{font-size:2em;width:20vw;height:9vh;margin-left:2vw;margin-top:4vh}
	.left_p{width:20vw;text-align:center;color:#FFF;font-size:1.3em;height:6vh}
	.left_p span{line-height:6vh}
	.left_p:hover{background:#ec3b3b;}
	.left_pAct{background:#ec3b3b;}
	.left_content{width:75vw;height:85vh;margin-left:2.5vw;margin-top:7vh;border-radius:10px;color:#FFF}
	.left_user{width:79vw;height:100vh;float:left;}
	
	.seller{width:10vw;height:6vh; border:1px;background:#ec3b3b;font-size:1.8em; color:#FFF;border-radius:10px 10px 0px 0px;margin-left:1vw;box-shadow:0px 0px  3px 3px #ec3b3b}
	.goods{width:10vw;height:6vh; border:1px;background:#3D9972;font-size:1.4em; color:#FFF;border-radius:10px 10px 0px 0px;margin-left:1vw;}
	.font{font-size:1.4em; line-height:1.5em}
	.content{width:75vw;height:78vh;border-top:3px solid #3D9972; overflow:hidden;}
	.touristImg{width:75vw;height:78vh;border-top:3px solid #3D9972; overflow:hidden;}
	.last_page{width:70vw; text-align:center;margin-top:2vh;font-size:1.4em;color:#000}
	.touristimg1{width:35.5vw;height:34vh;border:0.5em solid #FFF;float:left}
	.touristimg2{width:35.5vw;height:34vh;border:0.5em solid #FFF;float:right}
	.touristimg3{width:35.5vw;height:34vh;border:0.5em solid #FFF;float:left;margin-top:4vh}
	.touristimg4{width:35.5vw;height:34vh;border:0.5em solid #FFF;float:right;margin-top:4vh}
	.timg{width:35.5vw;height:34vh}