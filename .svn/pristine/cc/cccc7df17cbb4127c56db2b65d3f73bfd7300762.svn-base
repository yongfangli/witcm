
#b-top{
	width:1200px;
	height: 125px;
	margin: 0 auto;
	background-color: #e9e9e9;
	border-radius: 5px;
	margin-top:0px;
	
}
#b-top .div_first{
	width: 1200px;
	margin: 0 auto;
	height: 60px;
	padding-bottom: 5px;
	border-bottom: 1px dotted #333;
}
#b-top .div_first span{
	font-size: 16px;
	font-family: "微软雅黑" "Regular";
	color:#333;
	display: inline-block;
	padding: 0px 10px;
	margin-top: 20px;
	width: 95px;
	text-align: center;
}
#b-top .div_first span:HOVER{
	cursor: pointer;
}
#b-top .div_first .all{
    width: 95px;;
	line-height: 25px;
	font-size: 14px;
	font-family: "微软雅黑" "Regular";
	background-color: #EC3B3B;
	text-align: center;
	color: white;
}
#b-top .div_last{border-bottom:0px;}
#b-top .div_first .span_first{width: 100px;text-align: right;}
.shop-list{
	width:1200px;
	margin:0 auto;
	/*position:relative;*/
}
.content .div_img img{
	width: 80px;
	height: 80px;
	border-radius: 50%;
}
.content{overflow: hidden;margin-top:10px}
.content div{float: left;}

.content_info{margin-left:20px;color: #666;margin-bottom: 20px;}
.content_info a{
	display: block;
	width: 120px;
	line-height: 35px;
	font-size: 16px;
	font-family: "微软雅黑" "Regular";
	color: white;
	background-color: #666;
	margin-top:5px;
	text-decoration: none;
	border-radius: 5px;
	text-align: center;
}
.content_info a:hover{background-color: #EC3B3B;}
.content_info img{vertical-align: middle;}
.content_info span{
	font-size: 20px;
	font-family: "微软雅黑" "Regular";
}
.b-title{
	font-size: 20px;
	font-family: "微软雅黑" "Regular";
	color: #333;
}
.content_info span.wujiaoxingR{
	color:#EC3B3B;
	
}
.wujiaoxingR span{
	color:#EC3B3B;
	margin-left:3px;
}
.content_info span.wujiaoxingG{
	color:#666;
}
.content hr{color: #ccc;clear: both;}
#page{text-align: center;margin-top:60px;}
#page a{
	display: inline-block;
	font-size: 12px;
	text-decoration: none;
	color: #000;
}
.a_2{
	border: 1px solid #cccccc;
	border-radius: 2px;
	padding: 0px 10px;
}
#page .a_2:hover{
	color:#fff;
	background-color: #EC3B3B;
}