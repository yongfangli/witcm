

#dvContent {
    width: 1200px;
    margin: 0px auto;
	margin-top:30px;
	overflow:hidden
}
#page{text-align: center;margin-top:60px;}
#page a{
	display: inline-block;
	font-size: 12px;
	text-decoration: none;
	color: #000;
}
.a_2{
	border: 1px solid #cccccc;
	border-radius: 2px;
	padding: 0px 10px;
}
#page .a_2:hover{
	color:#fff;
	background-color: #EC3B3B;
}
.shapping{width:100%;height:164px;}
.market_logo{float:left;width:120px;height:100%;text-align:center}
.logo{border-radius:50px;width:80px;height:80px;}
.collect{margin-top:15px;width:120px;height:35px;background:#ec3b3b;border-radius:5px;border:1px;color:#FFF}
.browse{vertical-align: middle;width:30px;height:20px}
.market_con{font-size:12px;color:#333;display:inline;}
.mar_jiao{float:right;width:1050px;height:100%;}
.chaoshiming{font-size:20px;color:#333}
.mar_tem{margin-top:10px;font-size:16px;color:#666;}
.add_mar{font-size:16px;color:#666;}
.mar_con{font-size:16px;color:#666; line-height:25px;margin-top:10px;}
.review{width:1200px;margin:0px auto;margin-top:40px}
.communt{width:115px;height:30px;background:#999;color:#FFF;margin-left:10px;border-radius:5px 5px 0px 0px;border:1px}
.tiesre{width:115px;height:30px;color:#FFF;margin-left:5px;border-radius:5px 5px 0px 0px;border:1px}
.review_div{width:100%;border:1px solid #ccc;border-radius:5px}
.commu_img{width:96%;border-bottom:1px dashed #666;margin:0px auto; margin-top: 53px;}
.commu_imgs{width:96%;border-bottom:1px dashed #666;margin:0px auto;margin-top:15px}
.commu_img_logo{border-radius:50px;width:60px;height:60px;}
.right_con{width:91%;margin-left:100px;margin-top:-60px}
.grade{color:#ec3b3b; font-size:20px}
.com_name{font-size:14px;color:#ccc;margin-left:30px}
.con_text{font-size:16px;color:#666;margin-top:10px}
.goods img{
	width: 260px;
	height: 200px;
}
input[type=button]:HOVER {
	cursor: pointer;
}