* {
    margin: 0px;
    padding: 0px;
}

#dvMain {
    width: 100%;
    min-height: 690px;
    background: #e9e9e9;
}

#dvContent {
    width: 1200px;
    margin: 0px auto;
}

/********************左边菜单主容器*********************/
#dvContentLeft {
    float: left;
    width: 150px;
    margin-top:30px;
    min-height: 660px;
    background: #999999;
}

#dvContentLeft ul li {
    list-style: none;
    margin-top: 1px;

}

#dvContentLeft ul li a {
    background: #ec2b2b;
    color: #fff;
}

#dvCheck {
    background: #ec3b3b;
}

#dvContentLeft ul li a div {
    background: #20292b;
    width: 150px;
    height: 50px;
    line-height: 50px;
    text-align: center;
    font-family: 微软雅黑 Regular;
    font-size: 18px;
    color: #cccccc;
}

#dvContentLeft ul li a:link, a:hover {
    text-decoration: none;
    color: #cccccc;
}

#dvContentLeft ul li a #dvCheck {
    background: #ec3b3b;
    color: #fff;
}

/**********************右边主容器**********************/
#dvContentRight {
    float: left;
    width: 988px;
    min-height: 660px;
    margin-top: 30px;
    margin-left: 30px;
    margin-right: 30px;
    border-radius: 4px;
    box-shadow: 0px 0px 15px 1px #c2c2c2;
    background: #fff;
}

#dvContentRight #dvInner {
    width: 100%;
    margin-top: 60px;
}

#dvContentRight #dvInner table {
    width: 920px;
    margin: 0px auto;
}

/*标题列*/
#dvContentRight #dvInner table td.title {
    width:70px;
    height: 35px;
    padding: 10px;
    font-size: 14px;
    color: #4e4e4e;
    font-family: 宋体;
    font-weight: bold;
    text-align: right;
}

#dvContentRight #dvInner table td input
{
    height:35px;
    border-radius: 8px;
    text-align:center;
    border:0px;
}

#dvContentRight #dvInner table td #txtCount
{
    width:700px;
    background:#45ab83;
}

#dvContentRight #dvInner table td #txtRest
{
    width:520px;
    background:#f16f71;
}

#dvContentRight #dvInner table td #txtOut
{
    width:180px;
    background:#fed472;
}

#dvContentRight #dvInner table td.tb_btn {
    text-align: center;
    padding-top: 100px;
}

#dvContentRight #dvInner table button.conis {
    width: 260px;
    height: 40px;
    background: #ec3b3b;
    border-radius: 10px;
    color: #feffff;
	border:1px;
}
