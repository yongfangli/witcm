@charset "utf-8";
/*reset*/
html {
	font-size: 312.5%;
}
body {
	font-family: Microsoft Yahei,'roboto', helvetica, arial, sans-serif;
	font-size: .28rem;
	color: #333333;
}
body, h1, h2, h3, h4, h5, h6, hr, p, blockquote, dl, dt, dd, ul, ol, li, pre, form, fieldset, legend, button, input, textarea, th, td {
	margin: 0;
	padding: 0;
}
table {
	border-collapse: collapse;
	border-spacing: 0
}
img {
	vertical-align: middle;
	border: 0
}
h1, h2, h3, h4, h5, h6 {
	font-weight: normal;
}
ul, ol, li {
	list-style: none;
}
em, i {
	font-style: normal
}
img {
	border: none;
}
input, img {
	vertical-align: middle
}
input, select, textarea {
	-webkit-tap-highlight-color: rgba(0,0,0,0);
	-webkit-appearance: none;
	border: 0;
	border-radius: 0;
}
textarea:focus {
	outline: 0
}
a {
	color: #6E7D8B;
	text-decoration: none;
	outline: 0
}
a:active, a:hover, a.current {
	outline: 0
}
html {
	font-size: 312.5%;
}
img {
	border: none;
	display: block
}
input {
	border: none;
	font-size: .28rem;
}
input:focus {
	outline: none;
}


/*regsiter*/

 @-webkit-keyframes rotate {
 from {
-webkit-transform:rotate(0deg);
}
 to {
-webkit-transform:rotate(360deg);
}
}

@media screen and (min-width:350px) {
html {
	font-size: 342%;
	
}
}

@media screen and (min-width:410px) {
html {
	font-size: 400%;
}
}

@media screen and (min-width:480px) {
html {
	font-size: 469%;
}
}

@media screen and (min-width:640px) {
html {
	font-size: 625%;
}
}


.wrap_box {
	max-width:640px;
	margin: 0 auto;
	height:auto;
	position:relative;
	overflow:hidden;
	background:#E8E8E8
}
.top{width:100%;height:190px;background:url(../images/topback.png);background-size: 100% 190px;}
.return{color:#FFF;font-size:1.2em; margin-left:3%;padding-top:2vh}
.top img{width:17%;border:8px solid #fff;border-radius:10em;margin:0px auto;margin-top:1.5em}
.welcome{text-align:center;color:#FFF;margin-top:1em}
.login{border-bottom:1px solid #FFF}

.mylist{height:10vh;width:100%; background:#FFF;margin-top:0.5vh;font-size:1.3em}
.mylist img{width:12%;float:left;margin-top:1vh;margin-left:3%}
.userinfo{float:left; line-height:10vh;margin-left:3%}
.return2{float:right; line-height:10vh;;margin-right:3%;color:#666}

/***********页尾开始***************/
.footer_box{ width:100%;
			 height:1.4rem;
			 background:#efefef;
			 overflow:hidden; position: fixed; bottom:0;width:100%}
.footer_box a{ display:block;
               width:33%;
			   height:1.1rem;
			   color:#000;
			   float:left;
			   font-size:.29rem;
			   text-align:center;}
.footer_box a:active{ background:#013c0c;}			   
.footer_box a img{ display:block;
                   width:40%;
				   margin:0 auto .05rem;}
/***********页尾结束***************/