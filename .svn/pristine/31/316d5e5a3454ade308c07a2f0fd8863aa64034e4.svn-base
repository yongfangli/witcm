@charset "utf-8";
#dvMain {
    width: 100%;
    min-height: 600px;
    background: #e9e9e9;
}

#dvContent {
    width: 100%;
    min-height: 600px;
    margin: 0px auto;
}
.nav_box{ width:90%;
		  padding:0 5%;
		  overflow:hidden;
		  box-shadow:0 2px 4px #999;
		  margin-bottom:.06rem;
		  z-index:100;}
.back_but{ display:block;
           width:10%;
		   height:.26rem;
		   float:left;
		   margin-top:.12rem;
		  color: #6E7D8B;
    text-decoration: none;
    outline: 0;
		   }
.back_but img{ width:100%;
                display:block;}
.nav_title{ float:left;
			width:80%;
			color:#096;
			text-align:center;
			line-height:.7rem;
		   font-size: 0.8rem;}
/**********************内容**********************/
#dvInner {
    width: 100%;
    margin-top: 30px;
    height: 440px;
    border-radius: 4px;
    box-shadow: 0px 0px 15px 1px #c2c2c2;
    background: #fff;
    overflow: hidden;
}

#dvInner table {
    width: 880px;
 }
#dvInner table tr{
	height: 45px;
}

#dvInner table tr td a{
	color:red;
	cursor: pointer;
	text-decoration: none;
}

/*标题列*/
#dvInner table td.title {
    height: 35px;
    width:90px;
    padding: 5px;
    font-size: 16px;
    color: #555;
    font-family: 宋体;
    font-weight: bold;
    text-align: right;
}
#dvInner table td span {
    color:red;
}

/************文本框 下拉框 多行文本框************/
#dvInner table select, input {
    font-size: 14px;
    color: #696969;
    font-family: 宋体;
    border-radius: 5px;
    box-shadow: 0px 0px 1px 1px #c2c2c2;
	border:1px;
}

#dvInner table input {
    width: 200px;
    height: 35px;
}
#dvInner .selectOffice input{
	width: 200px;
	background-color: #f3f3f3;
}
#dvInner table input[name="validateCode"]{
	width: 120px;
}
/****************按扭样式******************/
#dvContent #dv_btn {
    text-align: center;
    padding-top: 20px;
}

#dvContent #dv_btn button.submit {
    width: 200px;
    height: 40px;
    background: #ec3b3b;
    border-radius: 10px;
    color: #feffff;
	border:1px;
	outline: none;
	font-size: 20px;
}
#dvContent #dv_btn button.submit:HOVER{
	cursor: pointer;
}

#dvContent #dv_btn button.reset {
    width: 200px;
    height: 40px;
    background: #20292b;
    border-radius: 10px;
    color: #feffff;
	border:1px;
	outline: none;
	font-size: 20px;
	margin-bottom: 15px;
	margin-top: 10px;
}
#dvContent #dv_btn button.reset:HOVER {
	cursor: pointer;
}