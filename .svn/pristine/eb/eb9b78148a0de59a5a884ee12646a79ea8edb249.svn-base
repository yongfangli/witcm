@charset "utf-8";
/**********************右边主容器**********************/

#dvContentRight #dvInner {
    width: 100%;
    margin-top: 60px;
}

#dvContentRight #dvInner table {
    width: 920px;
    margin: 0px auto;
}

#dvContentRight #dvInner table td {
    height: 50px;
}

/*标题列*/
#dvContentRight #dvInner table td.title {
    height: 35px;
    padding: 10px;
    font-size: 14px;
    color: #4e4e4e;
    font-family: 宋体;
    font-weight: bold;
    text-align: right;
}

#dvContentRight #dvInner table td input
{
    height:35px;
    border-radius: 8px;
    text-align:center;
    border:0px;
}

#dvContentRight #dvInner table td #txtCount
{
    width:700px;
    background:#45ab83;
}

#dvContentRight #dvInner table td #txtRest
{
    width:520px;
    background:#f16f71;
}

#dvContentRight #dvInner table td #txtOut
{
    width:180px;
    background:#fed472;
}

#dvContentRight #dvInner table td.tb_btn {
    text-align: center;
    padding-top: 100px;
}

#dvContentRight #dvInner table button.conis {
    width: 260px;
    height: 40px;
    background: #ec3b3b;
    border-radius: 10px;
    color: #feffff;
	border:1px;
}